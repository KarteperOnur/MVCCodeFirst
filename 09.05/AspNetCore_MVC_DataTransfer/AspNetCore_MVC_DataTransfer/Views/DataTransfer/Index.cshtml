@model AspNetCore_MVC_DataTransfer.Models.Person

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Data Transfer</h1>

<div class="row">
    <div class="col-md-6">
        <h2>
            @ViewBag.Person
        </h2>
    </div>
    <div class="col-md-6">
        <h2>@ViewData["Company"]</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <h2>@Model.Firstname @Model.Lastname</h2>
    </div>
</div>

