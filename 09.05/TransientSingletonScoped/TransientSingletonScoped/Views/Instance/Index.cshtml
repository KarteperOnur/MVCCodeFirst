@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-hover">
    <thead>
        <tr>
            <th>Singleton</th>
            <th>Scope</th>
            <th>Transient</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@ViewBag.Singleton1</td>
            <td>@ViewBag.Scope1</td>
            <td>@ViewBag.Transient1</td>
        </tr>
        <tr>
            <td>@ViewBag.Singleton2</td>
            <td>@ViewBag.Scope2</td>
            <td>@ViewBag.Transient2</td>
        </tr>

    </tbody>
</table>