@model List<AspNetCore_MVCLayout.Models.Category>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Home/Index</h1>
    <div>
        <table>
            <tr>
                <th>Category Name</th>
            </tr>
           @foreach (var item in Model)
            {
                <tr>
                    <td>@item.CategoryName</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
